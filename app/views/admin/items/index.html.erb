 <div class='row'>
   <div class='col-md-8 offset-md-1'>
     <h2>商品一覧</h2>
     <table class="table table-hover table-inverse">
       <thead>
         <tr>
          <th>商品id</th>
          <th>商品名</th>
          <th>商品価格</th>
          <th>商品ジャンル</th>
          <th>販売ステータス</th>
         </tr>
       </thead>
       <tbody>
         <% @items.each do |item| %>
           <tr>
             <td><%= item.id %></td>
             <td><%= link_to item.name, admin_item_path(item.id) %></td>
             <td><%= item.price %>円</td>
             <td><%= item.genre.name %></td>
             <td><%= item.status %></td>
           </tr>
         <% end %>
       </tbody>
     </table>
   </div>
 </div>