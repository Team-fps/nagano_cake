<main>
  <p id="notice"></p>
  <div class='container'>
    <div class='row'>
      <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0 mx-auto">
        <h1>ジャンル編集</h1>
        <%= form_with model:@genre, url:"/admin/genres/#{@genre.id}", local:true do |f| %>
          <%= f.text_field :name %>
          <%= f.submit '変更を保存', class:"btn btn-success" %>
        <% end %>
      </div>
    </div>
  </div>
</main>