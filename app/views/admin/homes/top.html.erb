<div class="offset-md-1">
  <h2 class="customers my-4 px-1 text-center">会員一覧</h2>
  <table class="table">
    <thead>
      <tr>
        <th>購入日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
    </thead>
    <tbody>
      <% @customers.each do |customers| %>
      <tr>
        <td>cart_itemの作成日時(購入日時)</td>
        <td>
          <%= link_to admin_customer_path(customers.id) do %>
            <%= customers.first_name %> <%= customers.last_name %>
          <% end %>
        </td>
        <td><%= customers.cart_items.count %></td>
        <td>注文ステータス(order_status)</td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
<%= paginate @customers %>
